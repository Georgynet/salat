meta {
  name: Register random User
  type: http
  seq: 7
}

post {
  url: http://localhost:8080/api/register
  body: json
  auth: none
}

script:pre-request {
  const {faker} = require('@faker-js/faker')
  
  const name = faker.name.firstName()
  
  req.setBody({
    "username": name,
    "password": name
  })
}
